{
  "name": "ace-core",
  "version": "0.5.0",
  "description": "Ace by DAISY, an Accessibility Checker for EPUB",
  "keywords": [
    "a11y",
    "accessibility",
    "ace",
    "checker",
    "daisy",
    "epub",
    "validator",
    "wcag"
  ],
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://opensource.org/licenses/MIT"
    }
  ],
  "author": {
    "name": "DAISY developers",
    "organization": "DAISY Consortium",
    "url": "https://github.com/daisy/"
  },
  "homepage": "https://github.com/daisy/ace",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daisy/ace.git"
  },
  "bugs": {
    "url": "https://github.com/daisy/ace/issues"
  },
  "engines": {
    "node": ">=6",
    "npm": ">=5",
    "yarn": ">= 1.0.0"
  },
  "main": "dist/index.js",
  "bin": {
    "ace": "dist/cli/cli.js",
    "ace-http": "dist/http/http.js"
  },
  "files": [
    "LICENCE.txt",
    "README.md",
    "dist"
  ],
  "dependencies": {
    "axe-core": "^2.4.2",
    "env-paths": "^1.0.0",
    "express": "^4.15.5",
    "express-easy-zip": "^1.1.4",
    "extract-zip": "^1.6.5",
    "filehound": "^1.16.2",
    "fs-extra": "^4.0.2",
    "h5o": "^0.11.3",
    "marko": "^4.4.26",
    "meow": "^3.7.0",
    "multer": "^1.3.0",
    "nightmare": "^2.10.0",
    "path": "^0.12.7",
    "shortid": "^2.2.8",
    "snyk": "^1.42.7",
    "tmp": "^0.0.33",
    "unzip": "^0.1.11",
    "uuidv4": "^0.5.0",
    "winston": "^2.4.0",
    "xmldom": "^0.1.27",
    "xpath": "^0.0.24"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.26.0",
    "cross-spawn": "^5.1.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.3.0",
    "jest": "^21.1.0",
    "rimraf": "^2.6.1",
    "standard-version": "^4.2.0",
    "uglify-js": "^3.0.8",
    "watch": "^1.0.2"
  },
  "resolutions": {
    "nightmare/electron": "1.7.8"
  },
  "scripts": {
    "start": "node src/cli/cli.js",
    "clean": "rimraf dist && mkdir dist",
    "prebuild": "npm run clean",
    "build": "npm run build:js",
    "build:js": "babel src -d dist --copy-files --ignore *.test.js",
    "build:scripts": "babel src -d dist --presets browsers --only src/scripts",
    "build:watch": "watch 'npm run build' src",
    "lint": "eslint src",
    "test": "jest",
    "docs": "echo add docs",
    "protect": "snyk protect",
    "prepare": "npm run protect",
    "prepublishOnly": "npm run build",
    "release": "standard-version -s"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": [
              "6"
            ]
          }
        }
      ]
    ]
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/tests/jest-setup.js",
    "testEnvironment": "node"
  },
  "snyk": true
}
